{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@theme/*": [
        "./src/theme/*"
      ],
      "@storage/*": [
        "./src/storage/*"
      ],
      "@types/*": [
        "./src/@types/*"
      ]
    }
  }
}